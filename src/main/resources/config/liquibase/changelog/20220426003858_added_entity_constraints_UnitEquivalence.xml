<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity UnitEquivalence.
    -->
    <changeSet id="20220426003858-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="unit_id"
                                 baseTableName="unit_equivalence"
                                 constraintName="fk_unit_equivalence__unit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="unit"/>

        <addForeignKeyConstraint baseColumnNames="child_id"
                                 baseTableName="unit_equivalence"
                                 constraintName="fk_unit_equivalence__child_id"
                                 referencedColumnNames="id"
                                 referencedTableName="unit_equivalence"/>
    </changeSet>
</databaseChangeLog>
